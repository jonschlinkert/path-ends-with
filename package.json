{
  "name": "path-ends-with",
  "description": "Return `true` if a file path ends with the given string/suffix.",
  "version": "1.0.0",
  "homepage": "https://github.com/jonschlinkert/path-ends-with",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/path-ends-with",
  "bugs": {
    "url": "https://github.com/jonschlinkert/path-ends-with/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "ends-with": "^1.0.1",
    "normalize-path": "^2.1.1"
  },
  "devDependencies": {
    "gulp-format-md": "^0.1.12",
    "mocha": "^3.2.0"
  },
  "keywords": [
    "directory",
    "end",
    "ending",
    "ends",
    "ends-with",
    "file",
    "filepath",
    "fp",
    "match",
    "matches",
    "normalize",
    "path",
    "re",
    "regex",
    "regexp",
    "segment",
    "slash",
    "slashes",
    "suffix",
    "trailing",
    "with"
  ],
  "verb": {
    "related": {
      "list": [
        "contains-path",
        "ends-with",
        "normalize-path",
        "unixify"
      ]
    },
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
