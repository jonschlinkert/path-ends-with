{
  "name": "path-ends-with",
  "description": "Return `true` if a file path ends with the given string/suffix.",
  "version": "2.0.0",
  "homepage": "https://github.com/jonschlinkert/path-ends-with",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/path-ends-with",
  "bugs": {
    "url": "https://github.com/jonschlinkert/path-ends-with/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "devDependencies": {
    "gulp-format-md": "^1.0.0",
    "mocha": "^3.5.3"
  },
  "keywords": [
    "path",
    "ends-with",
    "matches",
    "match"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "contains-path",
        "ends-with",
        "normalize-path",
        "unixify"
      ]
    },
    "lint": {
      "reflinks": true
    }
  }
}
